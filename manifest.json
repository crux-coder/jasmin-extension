{
  "browser_action": {
    "browser_style": true,
    "default_title": "fcc-utility",
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "512": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Testing people on FreeCodeCamp.",
  "manifest_version": 2,
  "name": "FCC Utility",
  "permissions": [
    "tabs",
    "activeTab",
    "https://www.freecodecamp.org/*",
    "storage"
  ],
  "version": "0.1"
}